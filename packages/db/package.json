{
  "name": "@magazine/db",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "migrate:dev": "dotenv -e ../../.env -- prisma migrate dev",
    "migrate:deploy": "dotenv -e ../../.env -- prisma migrate deploy",
    "seed": "dotenv -e ../../.env -- ts-node --transpile-only prisma/seed.ts",
    "build": "prisma generate && tsc -p tsconfig.json"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "mysql2": "^3.17.0",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "bcryptjs": "^3.0.3"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "dotenv-cli": "^7.4.2"
  },
  "prisma": {
    "seed": "dotenv -e ../../.env -- ts-node --transpile-only prisma/seed.ts"
  }
}